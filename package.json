{
  "name": "tsuyoshi-dev",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.8.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "lint:biome": "biome check --apply ./src",
    "oxlint": "oxlint",
    "lint-css": "stylelint \"**/*.{css,astro}\" --ignore-path .gitignore",
    "lint-css:fix": "stylelint \"**/*.{css,astro}\" --fix --ignore-path .gitignore",
    "check": "astro check",
    "format": "biome format --write src && prettier --write '**/*.css'"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/mdx": "^3.1.0",
    "astro": "^4.10.1",
    "open-props": "^1.7.4",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.7.3",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^9.4.0",
    "eslint-plugin-astro": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.0",
    "prettier-plugin-astro": "^0.14.0",
    "remark-code-titles": "^0.1.2",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-pretty-order": "^0.5.1",
    "stylelint-use-logical-spec": "^5.0.1"
  }
}
